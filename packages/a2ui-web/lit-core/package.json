{
  "name": "@a2ui-web/lit-core",
  "version": "0.8.2",
  "description": "A2UI 0.8 核心库（Webpack 5 兼容版本）",
  "author": "a2ui-web",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.longbridge-inc.com:2222/long-bridge-frontend/a2ui-component.git",
    "directory": "lit-core"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./Events": {
      "types": "./dist/events/events.d.ts",
      "import": "./dist/events/events.js"
    },
    "./Types": {
      "types": "./dist/types/types.d.ts",
      "import": "./dist/types/types.js"
    },
    "./Primitives": {
      "types": "./dist/types/primitives.d.ts",
      "import": "./dist/types/primitives.js"
    },
    "./Styles": {
      "types": "./dist/styles/index.d.ts",
      "import": "./dist/styles/index.js"
    },
    "./UI": {
      "types": "./dist/ui/ui.d.ts",
      "import": "./dist/ui/ui.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepare": "bun run build",
    "build": "tsc -b --pretty && node scripts/fix-json-imports.js && node scripts/downlevel-syntax.js",
    "dev": "tsc -b --pretty --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@lit-labs/signals": "^0.1.3",
    "@lit/context": "^1.1.4",
    "lit": "^3.3.1",
    "markdown-it": "^14.1.0",
    "signal-utils": "^0.21.1"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^18",
    "esbuild": "^0.24.2",
    "glob": "^10.3.10",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public",
    "registry": "https://gitlab.longbridge-inc.com/api/v4/projects/4872/packages/npm/"
  }
}
